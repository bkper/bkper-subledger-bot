{
  "id": "subledger-bot",
  "name": "Subledger Bot",
  "description": "Connect Books into Ledger-Subledger tree relationship",
  "logoUrl": "https://static.thenounproject.com/png/2318601-200.png",
  "website": "https://bkper.com/apps/subledger-bot",
  "ownerName": "Bkper",
  "ownerLogoUrl": "https://avatars.githubusercontent.com/u/11943086?v=4",
  "ownerWebsite": "https://bkper.com",
  "repoUrl": "https://github.com/bkper/subledger-bot",
  "repoPrivate": false,
  "deprecated": false,
  "clientId": "1090047264364-8d3kjg91okm80vifv3rn4crgoqr54k2j.apps.googleusercontent.com",
  "scopes": ["https://www.googleapis.com/auth/userinfo.email"],
  "webhookUrl": "https://us-central1-bkper-tax-trigger.cloudfunctions.net/prod",
  "events": [
    "TRANSACTION_POSTED",
    "TRANSACTION_DELETED",
    "TRANSACTION_RESTORED",
    "TRANSACTION_UPDATED"
  ],
  "propertiesSchema": {
    "group": {
      "keys": [
        "tax_excluded",
        "tax_included",
        "tax_description"
      ],
      "values": [
        "${transaction.description}",
        "${account.name}"
      ]
    },
    "account": {
      "keys": [
        "tax_rate",
        "tax_description"
      ],
      "values": [
        "${transaction.description}",
        "${account.name}"
      ]
    },
    "transaction": {
      "keys": [
        "tax_round",
        "tax_amount"
      ]
    }
  }
}